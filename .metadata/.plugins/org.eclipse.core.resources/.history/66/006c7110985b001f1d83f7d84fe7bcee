	<%
		session.setAttribute("num", 10);
	%>
	
	<h3>산술연산자</h3>
	${num + 5} <br>
	${num - 5} <br>
	${num * 5} <br>
	${num / 2} ${num div 2}<br>
	${num % 3} ${num mod 3}<br>
	
	<h3>비교연산자</h3>
	${num > 5} ${num gt 5} <br>
	${num < 20} ${num lt 20} <br>
	${num >= 10} ${num ge 10} <br>?
	
	${num == 10} ${num eq 10} <br>
	${num != 20} ${num ne 20} <br>
	
	<h3>논리연산자</h3>
	
	
	
